        // turn string into char array so we can iterate 
        char[] floors = instructions.toCharArray();
        // create a variable to hold the current floor
        int currentFloor = 0; 

        // iterates through the character array and inrecments or decrements the current floor
        for (int i = 0; i < instructions.length(); i++){
            
            if(floors[i] == '('){
                currentFloor++;    
            }
            else if(floors[i] == ')'){
                currentFloor--;
            }
        }
        // prints out the answer
        System.out.println(currentFloor);
    }
}
